@model Vidly.Models.Customer

@{
    ViewBag.Title = Model.Name;
}

<h2>@Model.Name</h2>

<div>
    <ul>

        <li>Membership Type: @Model.MembershipType.Name</li>

        @if (Model.Birthday.HasValue)
        {
            <li>Birthday: @Model.Birthday.Value.ToShortDateString()</li>
        }

    </ul>
</div>

